{
  "name": "hyakumeitan-database",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.7",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "scrape:all": "tsx ./lib/batch-scraper.ts",
    "geocode": "bun ./lib/geocode-shops.ts",
    "prepare": "husky",
    "kill-port": "kill -9 $(lsof -t -i:2950) 2>/dev/null || true",
    "dev": "bun run kill-port && next dev --turbopack -p 2950 & sleep 3 && open http://localhost:2950"
  },
  "dependencies": {
    "next": "^16.1.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^25.0.10",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "csv-writer": "^1.6.0",
    "playwright": "^1.58.0",
    "tsx": "^4.21.0",
    "tailwindcss": "^4.1.18"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
}
